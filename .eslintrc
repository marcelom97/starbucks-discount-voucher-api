{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-useless-escape": "off",
    "func-names": ["error", "never"],
    "consistent-return": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "new-cap": "off",
    "prefer-spread": "off",
    "prefer-rest-params": "off",
    "comma-dangle": "off"
  }
}
